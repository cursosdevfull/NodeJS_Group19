{
  "name": "nodejs_group19",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=development jest --coverage --passWithNoTests",
    "test:unit": "cross-env NODE_ENV=development jest test/unit/* --coverage=true --passWithNoTests",
    "test:unique": "cross-env NODE_ENV=development jest --coverage=false  --passWithNoTests",
    "test:integration": "cross-env NODE_ENV=development jest test/integration/* --coverage=true --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=development jest --watchAll",
    "test:config": "cross-env NODE_ENV=development jest --showConfig",
    "test:clear": "cross-env NODE_ENV=development jest --clearCache",
    "test:coverage": "cross-env NODE_ENV=development jest --coverage",
    "test:coverage:unit": "npm run test:unit -- --coverage",
    "test:coverage:integration": "npm run test:integration -- --coverage",
    "clean": "rimraf dist cache reports",
    "build": "npm run clean & tsc & gulp minify",
    "build:prod": "npm run clean & tsc -p tsconfig.prod.json & gulp minify",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint",
    "lint:report": "eslint -f html -o reports/lint.html",
    "lint:fix": "eslint --fix",
    "start:dev": "nodemon src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cursosdevfull/NodeJS_Group19.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cursosdevfull/NodeJS_Group19/issues"
  },
  "homepage": "https://github.com/cursosdevfull/NodeJS_Group19#readme",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^5.2.1",
    "gulp": "^5.0.0",
    "gulp-uglify-es": "^3.0.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-html-reporter": "^3.10.2",
    "jest-html-reporters": "^3.1.7",
    "jest-sonar-reporter": "^2.0.0",
    "node-mocks-http": "^1.16.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "joi": "^17.13.3"
  }
}
